import bytes

def main() = {

  val b = bytes::empty(16);
  b.write(0, 104.toByte)
  b.write(1, 101.toByte)
  b.write(2, 108.toByte)
  b.write(3, 108.toByte)
  b.write(4, 111.toByte)
  println(b.show) // hello

  val b2 = b.slice(1, 3)
  println(b2.show) // ell

  b.write(1, 102.toByte)

  println(b2.show) // fll

  b2.write(0, 101.toByte)

  println(b.show) // hello
}