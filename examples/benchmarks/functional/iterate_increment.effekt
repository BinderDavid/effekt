import immutable/list
import immutable/option
import io/args
import text/string

def iterate(i: Int, a: Int) { f: Int => Int }: Int =
  if (i == 0) {
    a
  } else {
    iterate(i - 1, f(a)) { f }
  }

def run(n: Int) =
  iterate(n, 0) { x => x + 1 }

def main() = {
  val n = commandLineArgs().headOption.getOrElse { "" }.toInt.getOrElse { 5 };
  val r = run(n);
  println(r)
}