import immutable/list
import immutable/option
import io/args
import text/string

def fibonacci(i: Int): Int =
  if (i == 0) {
    0
  } else if (i == 1) {
    1
  } else {
    fibonacci(i - 1) + fibonacci(i - 2)
  }

def main() = {
  val n = commandLineArgs().headOption.getOrElse { "" }.toInt.getOrElse { 5 };
  val r = fibonacci(n);
  println(r)
}