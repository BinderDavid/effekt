effect Flip {
    def flip(): Boolean
}

effect Fail {
    def fail[A](): A
}

type Triple {
    Tiple(a: Int, b: Int, c: Int)
}

def hash_triple(triple): Int = { mod((53 * triple.a + 2809 * triple.b + 148877 * triple.c), 1000000007) }

def main() = {
    println(hash_triple(Triple(1,2,3)))
}
